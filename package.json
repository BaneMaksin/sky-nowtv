{
  "private": true,
  "homepage": "https://create-react-app-redux.now.sh",
  "engines": {
    "node": ">=16.15.1 <17",
    "npm": ">=8.11.0 <9"
  },
  "bin": {
    "sky-server": "bin/server.js",
    "sky-jest": "bin/tests.js"
  },
  "scripts": {
    "deploy": "now && now alias",
    "start:static:server": "npm run build:prod && node bin/server.js",
    "static:server": "node bin/server.js",
    "start": "webpack serve --env NODE_ENV=mocks",
    "build:prod": "webpack --env NODE_ENV=production --env MOCKS=true",
    "build": "webpack --env NODE_ENV=development",
    "stylelint": "stylelint \"./src/**/*.{scss,less,css}\" --config ./.stylelintrc",
    "eslint": "eslint --ext .jsx,.js,.tsx,.ts,.mjs .",
    "tsclint": "tsc --project ./tsconfig.json --noEmit",
    "lint": "npm run stylelint && npm run eslint && npm run tsclint",
    "test": "node bin/tests.js",
    "test:watch": "node bin/tests.js --watch",
    "test:build": "node bin/tests.js --only-build",
    "test:build:watch": "node bin/tests.js --only-build --watch",
    "test:build:delete": "node bin/tests.js --delete --only-build",
    "test:debug": "node bin/tests.js --no-cache --verbose",
    "test:debug:delete": "node bin/tests.js --no-cache --verbose --delete",
    "test:coverage:report": "node bin/tests.js --open-report",
    "test:update:snapshot": "node bin/tests.js --updateSnapshot",
    "cy:open": "cypress open --browser electron",
    "cy:headless": "cypress run --headless --browser electron",
    "pre-push": "yarn lint && yarn test && yarn build:prod && yarn ci:cy",
    "ci:cy": "start-server-and-test static:server http://localhost:3000 cy:headless"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "sanitize.css": "13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@webdiscus/pug-loader": "^2.7.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.2.4",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.3",
    "jest-summary-reporter": "^0.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "msw": "^0.42.3",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "pug": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "@babel/runtime": ">=7.18.6"
  }
}
